#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5c212673d4c0 .scope module, "tb_bcd_bin" "tb_bcd_bin" 2 1;
 .timescale 0 0;
v0x5c212678a510_0 .var "bcd", 7 0;
v0x5c212678a600_0 .net "binary", 6 0, v0x5c212678a230_0;  1 drivers
S_0x5c212673d650 .scope module, "uut" "bcd_bin" 2 8, 3 1 0, S_0x5c212673d4c0;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "bcd";
    .port_info 1 /OUTPUT 7 "binary";
v0x5c212673d830_0 .net "bcd", 7 0, v0x5c212678a510_0;  1 drivers
v0x5c212678a230_0 .var "binary", 6 0;
v0x5c212678a310_0 .var "ones_digit", 3 0;
v0x5c212678a3d0_0 .var "tens_digit", 3 0;
E_0x5c21267791b0 .event anyedge, v0x5c212673d830_0, v0x5c212678a3d0_0, v0x5c212678a310_0;
    .scope S_0x5c212673d650;
T_0 ;
    %wait E_0x5c21267791b0;
    %load/vec4 v0x5c212673d830_0;
    %parti/s 4, 4, 4;
    %store/vec4 v0x5c212678a3d0_0, 0, 4;
    %load/vec4 v0x5c212673d830_0;
    %parti/s 4, 0, 2;
    %store/vec4 v0x5c212678a310_0, 0, 4;
    %load/vec4 v0x5c212678a3d0_0;
    %pad/u 7;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %load/vec4 v0x5c212678a3d0_0;
    %pad/u 7;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %add;
    %load/vec4 v0x5c212678a310_0;
    %pad/u 7;
    %add;
    %store/vec4 v0x5c212678a230_0, 0, 7;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5c212673d4c0;
T_1 ;
    %pushi/vec4 151, 0, 8;
    %store/vec4 v0x5c212678a510_0, 0, 8;
    %delay 10, 0;
    %vpi_call 2 18 "$display", "BCD = %b, Binary = %b", v0x5c212678a510_0, v0x5c212678a600_0 {0 0 0};
    %pushi/vec4 88, 0, 8;
    %store/vec4 v0x5c212678a510_0, 0, 8;
    %delay 10, 0;
    %vpi_call 2 23 "$display", "BCD = %b, Binary = %b", v0x5c212678a510_0, v0x5c212678a600_0 {0 0 0};
    %pushi/vec4 18, 0, 8;
    %store/vec4 v0x5c212678a510_0, 0, 8;
    %delay 10, 0;
    %vpi_call 2 28 "$display", "BCD = %b, Binary = %b", v0x5c212678a510_0, v0x5c212678a600_0 {0 0 0};
    %delay 20, 0;
    %vpi_call 2 31 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x5c212673d4c0;
T_2 ;
    %vpi_call 2 35 "$dumpfile", "tb_bcd_bin.vcd" {0 0 0};
    %vpi_call 2 36 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5c212673d4c0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
